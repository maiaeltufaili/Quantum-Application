{% extends "base.njk" %}

{% block title %}Upload Circuit{% endblock %}

{% block content %}
    <h2>Upload Circuit JSON</h2>
    <form method="POST" action="/upload" enctype="multipart/form-data">
        <input type="file" name="circuitFile" accept="application/json" required>
        <button type="submit">Upload</button>
    </form>
{% endblock %}
